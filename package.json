    "idb": "^8.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.487.0",
    "next": "^14.2.26",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.0.1",
    "react-leaflet": "^4.2.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
