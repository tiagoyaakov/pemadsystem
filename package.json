{
  "name": "pemad-material-check-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:perf": "jest --testMatch='**/*performance.test.ts'",
    "test:e2e": "start-server-and-test start http://localhost:3000 playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "monitor:performance": "ts-node scripts/monitor-performance.ts",
    "monitor:performance:watch": "nodemon --exec ts-node scripts/monitor-performance.ts",
    "monitor:performance:report": "open performance-report.html",
    "monitor:performance:cleanup": "ts-node scripts/cleanup-reports.ts",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:complete": "node scripts/run-all-tests.js",
    "test:complete:quick": "node scripts/run-all-tests.js --skip-e2e --skip-load",
    "generate-pwa-assets": "node scripts/generate-pwa-icons.js",
    "pre-deploy-check": "node scripts/pre-deploy-check.js",
    "deploy": "npm run pre-deploy-check && vercel --prod",
    "deploy:preview": "vercel",
    "analyze-bundle": "node scripts/analyze-bundle.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
